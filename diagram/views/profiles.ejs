    <!-- START HEADER -->
    <header id="header" class="page-topbar">
        <!-- start header nav-->
        <div class="navbar-fixed">
            <nav class="cyan">
                <div class="nav-wrapper">

                    <ul class="left">
                      <li class="no-hover"><a href="#" data-activates="slide-out" class="menu-sidebar-collapse btn-floating btn-flat btn-medium waves-effect waves-light cyan"><i class="mdi-navigation-menu" ></i></a></li>

                      <li><h1 class="logo-wrapper"><a href="/" class="brand-logo darken-1">ER-Diagram</a> <span class="logo-text">Materialize</span></h1></li>
                    </ul>


                </div>
            </nav>
        </div>
        <!-- end header nav-->
